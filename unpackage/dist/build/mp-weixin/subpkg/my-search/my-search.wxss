.search-box{width:100%;position:fixed;top:0;background-color:red;z-index:100}.history-title-box{display:flex;justify-content:space-between;margin-top:120rpx}.history-title-box .history-title{display:block;margin-left:30rpx;font-size:32rpx}.history{display:flex;padding:30rpx;flex-wrap:wrap}.history .history-item{font-size:26rpx;margin:10rpx 16rpx;padding:10rpx 16rpx;background-color:#f6f6f6}.search-list{margin-top:120rpx}.search-list .search-item{position:relative;width:100%;display:flex;line-height:80rpx;height:80rpx;border-top:1px solid #d9d9d9}.search-list .search-item .search-icon{position:absolute;right:0}.search-list .search-item .search-text{width:95%;white-space:nowrap;overflow:hidden;text-overflow:ellipsis}.search-list .search-item:last-child{border-bottom:1px solid #d9d9d9}